{
    "name": "lazies",
    "description": "Lazy primitives and iterables.",
    "version": "0.8.0",
    "main": "dist/lib/index.js",
    "repository": "github:gregros/lazies",
    "scripts": {
        "test": "jest --coverage",
        "build": "tsc -b tsconfig.all.json",
        "watch": "tsc -b tsconfig.all.json --watch",
        "prepare": "husky install",
        "rebuild": "run-s clean build",
        "clean": "shx rm -rf dist",
        "lint:fix": "prettier --write src/**/*.ts && eslint --fix src/**/*.ts",
        "lint:check": "prettier --check src/**/*.ts && eslint src/**/*.ts"
    },
    "keywords": [
        "iterable",
        "seq",
        "async-iterable",
        "linq",
        "lodash",
        "lazy",
        "iterator",
        "functional",
        "sequence",
        "stream",
        "yield"
    ],
    "author": "GregRos",
    "license": "MIT",
    "lint-staged": {
        "*.{ts,js,mjs,mts,cjs,cts}": "eslint --cache --fix",
        "*.{ts,js,mts,mjs,cts,cjs,css,md,yml,yaml,json}": "prettier --write"
    },
    "devDependencies": {
        "@gregros/eslint-config": "^0.4.0",
        "@types/lodash": "^4.14.198",
        "@types/node": "^20.6.0",
        "@typescript-eslint/eslint-plugin": "^6.6.0",
        "@typescript-eslint/parser": "^6.6.0",
        "eslint": "^8.49.0",
        "husky": "^8.0.3",
        "lint-staged": "^14.0.1",
        "npm-run-all": "^4.1.5",
        "prettier": "^3.2.5",
        "shx": "^0.3.4",
        "ts-node": "^10.9.1",
        "tslib": "^2.6.2",
        "typescript": "^5.4.5"
    },
    "types": "dist/lib/index.d.ts",
    "packageManager": "yarn@4.1.1",
    "dependencies": {
        "@assertive-ts/core": "^2.1.0",
        "@types/jest": "^29.5.12",
        "jest": "^29.7.0",
        "jest-stare": "^2.5.1",
        "synchronous-promise": "^2.0.17",
        "ts-jest": "^29.1.2",
        "type-plus": "^7.6.2"
    }
}
