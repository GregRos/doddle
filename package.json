{
  "name": "doddle",
  "version": "7.0.0",
  "description": "Tiny, simple, and comprehensive (async) iteration toolkit.",
  "keywords": [
    "iteration",
    "iterable",
    "iterator",
    "generator",
    "lazy",
    "memoize",
    "async",
    "promise",
    "seq",
    "aseq",
    "thunk",
    "linq",
    "functional",
    "sequence",
    "stream",
    "yield",
    "rxjs",
    "lodash"
  ],
  "homepage": "https://github.com/GregRos/doddle#readme",
  "repository": "github:GregRos/doddle",
  "license": "MIT",
  "author": "GregRos",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      },
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    }
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/cjs/index.d.ts",
  "typings": "./dist/cjs/index",
  "files": [
    "README.md",
    "dist",
    "LICENSE.md",
    "doddle.md",
    "CHANGELOG.md",
    "src",
    "!**/tsconfig*",
    "!**/*tsbuild*"
  ],
  "scripts": {
    "build": "tsc -b .",
    "watch": "tsc -b . --watch",
    "prepare": "husky install",
    "build:clean": "run-s clean build",
    "clean": "shx rm -rf dist .buildinfo || true",
    "size:test": "size-limit",
    "prettier:fix": "prettier --write .",
    "eslint:fix": "eslint 'src/**/*.ts' --fix",
    "eslint:check": "eslint 'src/**/*.ts'",
    "prettier:check": "prettier --check .",
    "lint:fix": "run-s prettier:fix eslint:fix",
    "lint:check": "run-s eslint:check prettier:check",
    "bundle": "esbuild dist/esm/index.js --bundle --minify --outfile=bundle-dist/bundle.js",
    "test": "jest && yarn size:test",
    "test:coverage": "jest --coverage",
    "package": "node tools/packaging.cjs",
    "tidy": "yarn install && run-s lint:fix build:clean test package"
  },
  "dependencies": {
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@gregros/eslint-config": "^0.6.0",
    "@size-limit/esbuild": "^11.2.0",
    "@size-limit/esbuild-why": "^11.2.0",
    "@size-limit/preset-small-lib": "^11.2.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.7",
    "@types/shelljs": "^0.8.15",
    "@typescript-eslint/eslint-plugin": "^7.6.0",
    "@typescript-eslint/parser": "^7.6.0",
    "declare-it": "^3.0.7",
    "esbuild": "^0.25.8",
    "eslint": "^8",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^14.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "prettier-plugin-jsdoc": "^1.3.2",
    "prettier-plugin-organize-imports": "^3.2.4",
    "prettier-plugin-packagejson": "^2.5.0",
    "shelljs": "^0.8.5",
    "shx": "^0.3.4",
    "size-limit": "^11.2.0",
    "ts-jest": "^29.2.4",
    "typedoc": "^0.28.5",
    "typedoc-material-theme": "^1.4.0",
    "typedoc-plugin-mdn-links": "^5.0.6",
    "typescript": "^5.5.4",
    "yamprint": "^0.8.4"
  },
  "size-limit": [
    {
      "path": "dist/esm/seq.js",
      "limit": "5 kB",
      "gzip": true
    },
    {
      "path": "dist/esm/index.js",
      "limit": "6.25 kB",
      "gzip": true
    }
  ]
}
