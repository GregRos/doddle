{
  "name": "lazies",
  "description": "The laziest of them all.",
  "version": "0.6.1",
  "main": "dist/lib/index.js",
  "repository": "github:gregros/lazies",
  "scripts": {
    "test": "jest --coverage",
    "build": "tsc -b tsconfig.all.json",
    "prepare": "husky install",
    "clean": "shx rm -rf dist",
    "rebuild": "run-s clean build",
    "lint:fix": "prettier --write src/**/*.ts && eslint --fix src/**/*.ts",
    "lint:check": "prettier --check src/**/*.ts && eslint src/**/*.ts"
  },
  "keywords": [
    "iterable",
    "seq",
    "async-iterable",
    "linq",
    "lodash",
    "lazy",
    "iterator",
    "functional",
    "sequence",
    "stream",
    "yield"
  ],
  "author": "GregRos",
  "license": "MIT",
  "lint-staged": {
    "*.{ts,js,mjs,mts,cjs,cts}": "eslint --cache --fix",
    "*.{ts,js,mts,mjs,cts,cjs,css,md,yml,yaml,json}": "prettier --write"
  },
  "devDependencies": {
    "@gregros/eslint-config": "^0.4.0",
    "@types/jest": "^29.5.4",
    "@types/lodash": "^4.14.198",
    "@types/node": "^20.6.0",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "eslint": "^8.49.0",
    "husky": "^8.0.3",
    "jest": "^29.6.4",
    "lint-staged": "^14.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "shx": "^0.3.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "types": "dist/lib/index.d.ts"
}
