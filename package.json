{
  "name": "doddle",
  "version": "1.0.0-preview",
  "description": "Tiny yet comprehensive library for sync and async iteration and lazy evaluation. 100% test coverage, 1k+ tests.",
  "keywords": [
    "iteration",
    "iterable",
    "lazy",
    "thunk",
    "async",
    "linq",
    "lodash",
    "iterator",
    "functional",
    "sequence",
    "stream",
    "yield"
  ],
  "license": "MIT",
  "author": "GregRos",
  "type": "module",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      },
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    }
  },
  "main": "./dist/cjs/index.js",
  "types": "./dist/cjs/index.d.ts",
  "scripts": {
    "build": "tsc -b .",
    "watch": "tsc -b . --watch",
    "prepare": "husky install",
    "build:clean": "run-s clean build",
    "clean": "shx rm -rf dist || true",
    "prettier:fix": "prettier --write .",
    "eslint:fix": "eslint src/**/*.ts --fix",
    "eslint:check": "eslint src/**/*.ts",
    "prettier:check": "prettier --check .",
    "lint:fix": "run-s eslint:fix prettier:fix",
    "lint:check": "run-s eslint:check prettier:check",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "tidy": "yarn install && run-s lint:fix build:clean test"
  },
  "devDependencies": {
    "@gregros/eslint-config": "^0.6.0",
    "@types/eslint": "^8.56.9",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.14.198",
    "@types/node": "^20.12.7",
    "@types/prettier": "^3.0.0",
    "@types/shelljs": "^0.8.15",
    "@typescript-eslint/eslint-plugin": "^7.6.0",
    "@typescript-eslint/parser": "^7.6.0",
    "declare-it": "^2.0.2",
    "eslint": "^8",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^14.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "prettier-plugin-jsdoc": "^1.3.0",
    "prettier-plugin-organize-imports": "^3.2.4",
    "prettier-plugin-packagejson": "^2.5.0",
    "shelljs": "^0.8.5",
    "shx": "^0.3.4",
    "ts-jest": "^29.2.4",
    "typescript": "^5.5.4"
  },
  "packageManager": "yarn@4.2.2"
}
