{
  "name": "dawdle",
  "version": "3.0.1",
  "description": "Simple, flexible, and thoroughly tested library for iteration and lazy evaluation.",
  "keywords": [
    "iteration",
    "iterable",
    "lazy",
    "thunk",
    "async",
    "linq",
    "lodash",
    "iterator",
    "functional",
    "sequence",
    "stream",
    "yield"
  ],
  "license": "MIT",
  "author": "GregRos",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "node": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/index.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "tsc -b .",
    "watch": "tsc -b . --watch",
    "prepare": "husky install",
    "build:clean": "run-s clean build",
    "clean": "shx rm -rf packages/*/dist || true",
    "prettier:fix": "prettier --write .",
    "eslint:fix": "eslint ./packages/*/{lib,test}/**/*.ts --fix",
    "eslint:check": "eslint ./packages/*/{lib,test}/**/*.ts",
    "prettier:check": "prettier --check .",
    "lint:fix": "run-s eslint:fix prettier:fix",
    "lint:check": "run-s eslint:check prettier:check",
    "test": "yarn workspaces foreach -Apt run test",
    "tidy": "yarn install && run-s lint:fix build:clean test"
  },
  "resolutions": {
    "declare-it": "portal:/C:/codegr/declare-it"
  },
  "dependencies": {
    "@types/eslint": "^8.56.9"
  },
  "devDependencies": {
    "@assertive-ts/core": "^2.1.0",
    "@gregros/eslint-config": "^0.6.0",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.14.198",
    "@types/node": "^20.12.7",
    "@types/prettier": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^7.6.0",
    "@typescript-eslint/parser": "^7.6.0",
    "declare-it": "^1.1.0",
    "eslint": "^8",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^14.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "prettier-plugin-jsdoc": "^1.3.0",
    "prettier-plugin-organize-imports": "^3.2.4",
    "prettier-plugin-packagejson": "^2.5.0"
  },
  "packageManager": "yarn@4.2.2"
}
