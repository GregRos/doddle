{
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/__pycache__": true,
    "**/.idea": true,
    "**/.mypy_cache": true,
    "**/*.aux": true,
    "**/*.bbl": true,
    "**/*.fdb_latexmk": true,
    "**/*.fls": true,
    "**/*.log": true,
    "**/*.out": true,
    "**/*.synctex.gz": true,
    "**/dist_aux": true,
    "coverage": true,
    ".nyc_output": true,
    ".yarn": true
  },
  "typescript.tsserver.experimental.enableProjectDiagnostics": true,
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.patterns": {
    "*.ts": "${capture}.async.test.ts, ${capture}.sync.test.ts, ${capture}.test.ts, ${capture}.ctor.ts, ${capture}.class.ts",
    "*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
    "package.json": ".npmignore",
    "tsconfig.json": "tsconfig.esm.json, tsconfig.cjs.json, tsconfig.test.json",
    "tsconfig.base.json": "tsconfig.base.*.json",
    ".yarnrc.yml": "yarn.lock",
    ".eslintrc*": ".eslintignore",
    ".prettierrc*": ".prettierignore, .editorconfig",
    ".gitignore": ".gitattributes"
  }
}
