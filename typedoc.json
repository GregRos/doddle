{
  "plugin": ["typedoc-material-theme", "typedoc-plugin-mdn-links"],
  "entryPoints": ["src/index.ts"],
  "tsconfig": "src/tsconfig.esm.json",
  "out": "./typedoc",
  "navigation": {
    "includeCategories": true
  },
  "theme": "default",
  "defaultCategory": "Main",
  "kindSortOrder": ["Class", "Function", "TypeAlias", "Namespace"],
  "categoryOrder": ["Create", "Use", "Types"],
  "themeColor": "#210A4B",
  "favicon": "icon/favicon.ico"
}
