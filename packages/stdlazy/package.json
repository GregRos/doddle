{
  "name": "stdlazy",
  "version": "2.0.1",
  "description": "Flexible and debuggable lazy primitive.",
  "keywords": [
    "async",
    "promise",
    "sync",
    "initialization",
    "lazy",
    "lazy-evaluation",
    "functional",
    "map",
    "zip"
  ],
  "repository": "github:gregros/lazies",
  "license": "MIT",
  "author": "GregRos",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "node": "./dist/index.js"
    },
    "./utils": {
      "require": "./dist/utils.js",
      "types": "./dist/utils.d.ts",
      "import": "./dist/utils.js",
      "node": "./dist/utils.js"
    }
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/index.d.ts"
      ],
      "utils": [
        "./dist/utils.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "tsc -b .",
    "clean": "shx rm -rf dist",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "watch": "tsc -b . --watch",
    "build:clean": "run-s clean build"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.7",
    "jest": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.2"
  },
  "packageManager": "yarn@4.1.1"
}
