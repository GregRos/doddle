{
  "name": "stdseq",
  "version": "3.0.1",
  "description": "Simple, flexible, and strongly-typed iteration.",
  "keywords": [
    "iteration",
    "iterable",
    "seq",
    "async-iterable",
    "linq",
    "lodash",
    "iterator",
    "functional",
    "sequence",
    "stream",
    "yield"
  ],
  "license": "MIT",
  "author": "GregRos",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "node": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/index.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "tsc -b .",
    "build:clean": "yarn clean && yarn build",
    "clean": "shx rm -rf dist",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "watch": "tsc -b . --watch"
  },
  "resolutions": {
    "stdlazy": "portal:../stdlazy"
  },
  "dependencies": {
    "type-plus": "^7.6.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.7",
    "declare-it": "^1.1.0",
    "jest": "^29.7.0",
    "prettier-plugin-organize-imports": "^4.0.0",
    "shx": "^0.3.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.2"
  },
  "peerDependencies": {
    "stdlazy": "^2.0.0"
  },
  "packageManager": "yarn@4.1.1"
}
